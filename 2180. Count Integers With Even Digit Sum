class Solution {
public:
    int countEven(int num) {
        int c=0;
        for(int i=1;i<=num;i++){
            int n=i;
            int sum=0;
          while(n){
            int r=n%10;
            sum+=r;
            n=n/10;
        }
        if(sum%2==0){
        c++;
        }
        }
        return c;
    }
};
